!----------------------------------------
!    Color Settings
!----------------------------------------
! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc342b
*.color9:       #cc342b

! green
*.color2:       #198844
*.color10:      #198844

! yellow
*.color3:       #fba922
*.color11:      #fba922

! blue
*.color4:       #3971ed
*.color12:      #3971ed

! magenta
*.color5:       #a36ac7
*.color13:      #a36ac7

! cyan
*.color6:       #3971ed
*.color14:      #3971ed

! white
*.color7:       #c5c8c6
*.color15:      #ffffff

! Transparency
urxvt*background: rgba:0000/0000/0200/c800

!Highlighting folders
URxvt*dynamicColors:       true

!------------------------
! Xft settings
!------------------------

Xft.dpi:                    96
Xft.antialias:              false
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

URxvt*font: xft:Noto Mono for Powerline:pixelsize=14,style=regular,xft:icons-in-terminal:pixelsize=14
URxvt*boldFont: xft:Noto Mono for Powerline:bold:pixelsize=14,xft:icons-in-terminal:pixelsize=14
!Correct the underscore issue
URxvt.lineSpace:            1
!Fonts too separated
URxvt*letterSpace:          -1
!Allow bold fonts
URxvt.allow_bold:           true

!-------------------------
! URxvt settings
!-------------------------

URxvt.depth:                32

! Scroll
URxvt.saveLines:            65535
!URxvt.saveLines:            -1
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Perl extensions
URxvt.perl-ext-common:      default,matcher,tabbed,clipboard
URxvt.matcher.button:       1
URxvt.urlLauncher:          firefox

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #657b83
URxvt.cursorUnderline:      false
URxvt*mouseWheelScrollPage: false

! Pointer
URxvt.pointerBlank:         true

! Clipboard
!URxvt.keysym.M-C-c:              perl:clipboard:copy
!URxvt.keysym.M-v:                perl:clipboard:paste
!URxvt.keysym.M-C-v:              perl:clipboard:paste_escaped
!URxvt.clipboard.copycmd:  xsel --input  --clipboard
!URxvt.clipboard.pastecmd: xsel --output --clipboard
!URxvt.keysym.Control-Shift-c: perl:clipboard:copy
!URxvt.keysym.Control-Shift-v: perl:clipboard:paste
!URxvt.keysym.C-M-c: builtin-string:
!URxvt.keysym.C-M-v: builtin-string:
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.C-A-V: perl:clipboard:paste_escaped
URxvt.iso14755 : false
!urxvt*background: rgba:0000/0000/0200/c800

! Tabs
URxvt.tabbed.new-button:         true
URXvt.tabbed.reopen-on-close:    yes
URxvt.keysym.Control-Shift-t:    perl:tabbedex:new_tab
URxvt.keysym.Shifft-Right:       perl:tabbedex:next_tab
URxvt.keysym.Shift-Left:         perl:tabbedex:prev_tab
URxvt.tabbed.no-tabbedex-keys:   true

!URxvt*loginShell:                true

